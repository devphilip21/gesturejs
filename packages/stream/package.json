{
  "name": "@gesturejs/stream",
  "description": "A module for handling large data flows as streams within the gesturejs library.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./operators": {
      "import": "./dist/operators.js",
      "require": "./dist/operators.cjs"
    }
  },
  "scripts": {
    "build": "builder build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@gesturejs/builder": "workspace:*"
  },
  "devDependencies": {
    "vite": "*",
    "vitest": "^2.0.0",
    "jsdom": "^25.0.0",
    "@vitest/coverage-v8": "^2.0.0"
  }
}
