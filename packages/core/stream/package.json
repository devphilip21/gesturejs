{
  "name": "@cereb/stream",
  "description": "Lightweight, zero-dependency reactive stream library with RxJS-like pipe operators for cereb",
  "version": "0.2.0",
  "license": "MIT",
  "author": "devphilip21 <philip21.dev@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/devphilip21/cereb.git",
    "directory": "packages/core/stream"
  },
  "homepage": "https://github.com/devphilip21/cereb/tree/main/packages/core/stream",
  "bugs": {
    "url": "https://github.com/devphilip21/cereb/issues"
  },
  "keywords": [
    "stream",
    "reactive",
    "observable",
    "rxjs",
    "pipe",
    "functional-reactive",
    "event-stream",
    "operators",
    "cereb"
  ],
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./operators": {
      "types": "./dist/operators.d.ts",
      "import": "./dist/operators.js",
      "require": "./dist/operators.cjs"
    }
  },
  "scripts": {
    "build": "builder build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@cereb/builder": "workspace:^",
    "@vitest/coverage-v8": "*",
    "jsdom": "*",
    "vite": "*",
    "vitest": "*"
  }
}
